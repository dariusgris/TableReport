<Window x:Class="TableReport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TableReport"
        mc:Ignorable="d"
        Title="TableRender" Height="450" Width="800">
    <Grid x:Name="grid">
      <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20">
        <TextBox x:Name="inputString" Width="300" Height="25" FontSize="15" Margin="0,0,10,0" />
        <Button x:Name="submit" Width="150" Height="25" Content="Submit" Click="Button_Click" />
      </StackPanel>
    </Grid>
  
  
</Window>
